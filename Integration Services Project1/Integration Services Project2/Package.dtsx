                <component
                  refId="Package\Contenedor de bucles Foreach\Data Flow Task\Origen de ADO NET"
                  componentClassID="Microsoft.ManagedComponentHost"
                  contactInfo="Consume datos de SQL Server, OLE DB, ODBC u Oracle mediante el correspondiente proveedor de datos de .NET Framework. Se usa una instrucción Transact-SQL para definir el conjunto de resultados. Por ejemplo, se extraen datos de SQL Server con el proveedor de datos de .NET Framework para SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; Todos los derechos reservados; http://www.microsoft.com/sql/support;4"
                  description="Consume datos de SQL Server, OLE DB, ODBC u Oracle mediante el correspondiente proveedor de datos de .NET Framework. Se usa una instrucción Transact-SQL para definir el conjunto de resultados. Por ejemplo, se extraen datos de SQL Server con el proveedor de datos de .NET Framework para SQL Server."
                  name="Origen de ADO NET"
                  usesDispositions="true"
                  version="4">
                  <properties>
                    <property
                      dataType="System.String"
                      description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @FechaInicio char(50)
declare @FechaFin char(50)
/*************RANGO DE FECHAS DE FACTURACION****************************/

set @FechaInicio= '2021-05-01' /*-----------------FECHA DE INICIO*/
set @FechaFin=  '2021-05-06'  /*-----------------FECHA DE FINALIZACION*/  
/***************************************************************/

declare @tienda int
declare @nombretienda char(50)
declare @nombretiendafactura char(50)
select @nombretienda= fcTdaNombre from control
select @nombretiendafactura= fcTdaNombre from CATFISControl
select @tienda= fiNoTienda from control


/* '*************FACTURAS DE VENTA DE CONSUMO*********************'*/
select
DISTINCT  @tienda as 'Tienda',
factura.fccodigohash as 'CodigoHash',
 factura.fdcreacion as 'FechaCreacion',
factura.FCFOLIO,

case factura.fiestatusid
WHEN 0 THEN 'Pendiente'
WHEN 1 THEN 'Procesado Por Aplicacion'
WHEN 3 THEN 'En contingencia'
WHEN 4 THEN 'Procesado por Tarea'
ELSE 'No encontrado'
END as 'EstatusFactura',  

case factura.fitipoid
WHEN 1 THEN 'Factura'
WHEN 7 THEN 'Procesado Por Nota de Credito'

ELSE 'No encontrado'
END as 'TipoDocumento',  
@nombretienda as 'SucursalADN',
@nombretiendafactura as 'SucursalFacturacion',
factura.fcnoreferencia as 'VentaPedido',

case
WHEN pedido.FitipoVenta=1 THEN 'CONTADO'
WHEN pedido.FitipoVenta=2 THEN 'CREDITO'
ELSE 'No Encontrado'
END 'SurtimientoTipo',

pedido.fdFecSurt as 'FechaSurtimiento',
case pedido.fipedstat
WHEN 1 THEN  'SURTIDO'
WHEN 2 THEN 'CANCELADO ANTES DE ENTREGA'
WHEN 3 THEN 'CANCELADO DESPUES DE ENTREGA'
WHEN 4 THEN 'CONGELADO'
WHEN 5 THEN 'APARTADO'
ELSE 'NO SE ENCONTRO INFORMACION'
END 'EstadoPedido',


case
WHEN cliente.fcCteFisMor='1' OR cliente.fcCteFisMor= 'FI' THEN 'CLIENTE FISICO'
WHEN cliente.fcCteFisMor= 'MO' THEN 'EMPRESA'
ELSE 'No Encontrado'
END 'TipoPersona',

cliente.fcCteNombre as 'Nombres' ,
cliente.fcCteAPaterno as 'ApellidoPaterno' ,
cliente.fcCteAMaterno as 'ApellidoMaterno',
cliente.fcCteDirCalle as 'Direccion',
case factura.finegocioid
WHEN 1 THEN 'Consumo'
WHEN 2 THEN 'Tiempo de aire'
ELSE 'No encontrado'
END as 'TipoVenta',

tipo_operacion.fcTopDesc,
cliente.fcCteRFC as 'NIT',
producto.fiprodid,
detalle_pedido.fiCantArt,
producto.fcProdDesc,
detalle_pedido.fnPrcUnit,
detalle_pedido.fnPdctoDescto,
(detalle_pedido.fnPrcUnit-detalle_pedido.fnPdctoDescto)-((detalle_pedido.fnPrcUnit-detalle_pedido.fnPdctoDescto)/1.12) as "IVA",

case factura.finegocioid
WHEN 1 THEN (detalle_pedido.fnPrcUnit-detalle_pedido.fnPdctoDescto)
WHEN 2 THEN (ventatarjeta.fnimporte)
ELSE 'No Encontrado'
END  as 'TotalVenta',

moto.fcMarca,
moto.fcNoSerie,
moto.fcNoMotor,
moto.fcTipoMoto,
moto.fcNoCaja,
moto.fcColor,
moto.fcPedimento,
moto.fiAnio,
moto.fcmodelo,

case
when fcProdDesc like '%GTIA%' OR fcProdDesc like  '%SEGURO%' then 'S'
Else 'B'
END as 'BienOServicio'

from fisbitacoradocfiscal factura  FULL   JOIN pedido ON pedido.finopedido=factura.fcNoReferencia   FULL JOIN cliente ON
cliente.ficteid=pedido.ficteid and cliente.fiDigitoVer=pedido.fiDigitoVer FULL JOIN detalle_pedido ON detalle_pedido.finopedido=pedido.finopedido
FULL JOIN producto ON producto.fiprodid=detalle_pedido.fiprodid FULL JOIN surtimientomoto  moto ON moto.fiNoPedido=pedido.finopedido FULL JOIN control ON control.fiNoTienda=pedido.finotienda
 FULL JOIN ventatarjeta ON ventatarjeta.fcmin=factura.fcnoreferencia FULL JOIN movto_caja ON movto_caja.fiNoTransac=ventatarjeta.fiTranNo  FULL  JOIN tipo_operacion ON tipo_operacion.fitipoop=movto_caja.fitipoop
where  factura.finegocioid in(1) and  factura.fdCreacion between @FechaInicio AND @FechaFin  

/*'*************FACTURAS DE VENTA TIEMPO DE AIRE*********************'*/
select
DISTINCT  @tienda as 'Tienda',
factura.fccodigohash as 'CodigoHash',
factura.fdcreacion as 'FechaCreacion',
factura.FCFOLIO,
case factura.fiestatusid
WHEN 0 THEN 'Generado'
WHEN 1 THEN 'Procesado Por Aplicacion'
WHEN 3 THEN 'En contingencia'
WHEN 4 THEN 'Procesado por Tarea'
ELSE 'No encontrado'
END as 'EstatusFactura',  

case factura.fitipoid
WHEN 1 THEN 'Factura'
WHEN 7 THEN 'Procesado Por Nota de Credito'

ELSE 'No encontrado'
END as 'TipoDocumento',  
@nombretienda as 'SucursalADN',
@nombretiendafactura as 'SucursalFacturacion',
factura.fcnoreferencia as 'VentaPedido',
case
WHEN pedido.FitipoVenta=1 THEN 'CONTADO'
WHEN pedido.FitipoVenta=2 THEN 'CREDITO'
ELSE 'CONTADO'
END 'Surtimiento Tipo',

case
when pedido.fdFecSurt is null then fdcreacion
else pedido.fdFecSurt
END 'FechaSurtimiento',



case pedido.fipedstat
WHEN 1 THEN  'SURTIDO'
WHEN 2 THEN 'CANCELADO ANTES DE ENTREGA'
WHEN 3 THEN 'CANCELADO DESPUES DE ENTREGA'
WHEN 4 THEN 'CONGELADO'
WHEN 5 THEN 'APARTADO'
ELSE 'SURTIDO'
END 'EstadoPedido',

case
WHEN cliente.fcCteFisMor='1' OR cliente.fcCteFisMor= 'FI' THEN 'CLIENTE FISICO'
WHEN cliente.fcCteFisMor= 'MO' THEN 'EMPRESA'
ELSE 'CLIENTE FISICO'
END 'TipoPersona',

case
When cliente.fcCteNombre is NULL  Then 'Consumidor Final'
else cliente.fcCteNombre
end 'Nombre',


cliente.fcCteAPaterno as 'ApellidoPaterno',

cliente.fcCteAMaterno as 'ApellidoMaterno',

CASE
When cliente.fcCteDirCalle is NULL  Then 'CIUDAD'
else cliente.fcCteDirCalle
end 'Direccion',


case factura.finegocioid
WHEN 1 THEN 'Consumo'
WHEN 2 THEN 'Tiempo de aire'
ELSE 'No encontrado'
END as 'TipoVenta',

tipo_operacion.fcTopDesc,

CASE
When cliente.fcCteRFC  is NULL  Then 'CF'
else cliente.fcCteRFC
end 'NIT',


producto.fiprodid,


CASE
When detalle_pedido.fiCantArt  is NULL  Then 1
else detalle_pedido.fiCantArt
end 'fiCantArt',




CASE
When producto.fcProdDesc  is NULL  Then 'Tiempo de Aire'
else producto.fcProdDesc
end 'fcProdDesc',



((ventatarjeta.fnimporte)/1.12) as  'fnPrcUnit',

case  
when fnPdctoDescto is NULL then 0
end 'fnPdctoDescto',


(ventatarjeta.fnimporte) -((ventatarjeta.fnimporte)/1.12) as "IVA",

case factura.finegocioid
WHEN 1 THEN (detalle_pedido.fnPrcUnit-detalle_pedido.fnPdctoDescto)
WHEN 2 THEN (ventatarjeta.fnimporte)
ELSE 'No Encontrado'
END   'TotalVenta',

moto.fcMarca,
moto.fcNoSerie,
moto.fcNoMotor,
moto.fcTipoMoto,
moto.fcNoCaja,
moto.fcColor,
moto.fcPedimento,
moto.fiAnio,
moto.fcmodelo,

case
when fcProdDesc like '%GTIA%' OR fcProdDesc like  '%SEGURO%' then 'S'
Else 'B'
END as 'BienOServicio'

from fisbitacoradocfiscal factura  FULL   JOIN pedido ON pedido.finopedido=factura.fcNoReferencia   FULL JOIN cliente ON
cliente.ficteid=pedido.ficteid and cliente.fiDigitoVer=pedido.fiDigitoVer FULL JOIN detalle_pedido ON detalle_pedido.finopedido=pedido.finopedido
FULL JOIN producto ON producto.fiprodid=detalle_pedido.fiprodid FULL JOIN surtimientomoto  moto ON moto.fiNoPedido=pedido.finopedido FULL JOIN control ON control.fiNoTienda=pedido.finotienda


FULL JOIN  FISDocFiscalXTipoVenta  ON FISDocFiscalXTipoVenta.fcfolio=factura.fcfolio
 FULL JOIN ventatarjeta ON ventatarjeta.ficonsec=FISDocFiscalXTipoVenta.fiConsecOpeTA
 FULL JOIN movto_caja ON movto_caja.finotransac=ventatarjeta.fiTranNo INNER JOIN tipo_operacion ON tipo_operacion.fitipoop=movto_caja.fitipoop  

 
 where factura. fdCreacion between @FechaInicio AND @FechaFin  

/*'*************PEDIDOS SIN FACTURACION *********************'*/

select
DISTINCT @tienda as 'Tienda',
factura.fccodigohash as 'CodigoHash',
 factura.fdcreacion as 'FechaCreacion',
factura.FCFOLIO,
case factura.fiestatusid
WHEN 0 THEN 'Pendiente'
WHEN 1 THEN 'Procesado Por Aplicacion'
WHEN 3 THEN 'En contingencia'
WHEN 4 THEN 'Procesado por Tarea'
ELSE 'No encontrado'
END as 'EstatusFactura',  

case factura.fitipoid
WHEN 1 THEN 'Factura'
WHEN 7 THEN 'Procesado Por Nota de Credito'

ELSE 'No encontrado'
END as 'TipoDocumento',  
@nombretienda as 'SucursalADN',
@nombretiendafactura as 'SucursalFacturacion',
p.finopedido as 'VentaPedido',

case
WHEN p.FitipoVenta=1 THEN 'CONTADO'
WHEN p.FitipoVenta=2 THEN 'CREDITO'
ELSE 'No Encontrado'
END 'SurtimientoTipo',

p.fdFecSurt as 'FechaSurtimiento',
case p.fipedstat
WHEN 1 THEN  'SURTIDO'
WHEN 2 THEN 'CANCELADO ANTES DE ENTREGA'
WHEN 3 THEN 'CANCELADO DESPUES DE ENTREGA'
WHEN 4 THEN 'CONGELADO'
WHEN 5 THEN 'APARTADO'
ELSE 'NO SE ENCONTRO INFORMACION'
END 'EstadoPedido',


case
WHEN cliente.fcCteFisMor='1' OR cliente.fcCteFisMor= 'FI' THEN 'CLIENTE FISICO'
WHEN cliente.fcCteFisMor= 'MO' THEN 'EMPRESA'
ELSE 'No Encontrado'
END 'TipoPersona',

cliente.fcCteNombre as 'Nombres' ,
cliente.fcCteAPaterno as 'ApellidoPaterno' ,
cliente.fcCteAMaterno as 'ApellidoMaterno',
cliente.fcCteDirCalle as 'Direccion',
case factura.finegocioid
WHEN 1 THEN 'Consumo'
WHEN 2 THEN 'Tiempo de aire'
ELSE 'No encontrado'
END as 'TipoVenta',
tipo_operacion.fcTopDesc,
cliente.fcCteRFC as 'NIT',
producto.fiprodid,
dp.fiCantArt,
producto.fcProdDesc,
dp.fnPrcUnit,
dp.fnPdctoDescto,
(dp.fnPrcUnit-dp.fnPdctoDescto)-((dp.fnPrcUnit-dp.fnPdctoDescto)/1.12) as "IVA",

dp.fnPrcUnit-dp.fnPdctoDescto
  as 'TotalVenta',

moto.fcMarca,
moto.fcNoSerie,
moto.fcNoMotor,
moto.fcTipoMoto,
moto.fcNoCaja,
moto.fcColor,
moto.fcPedimento,
moto.fiAnio,
moto.fcmodelo,

case
when fcProdDesc like '%GTIA%' OR fcProdDesc like  '%SEGURO%' then 'S'
Else 'B'
END as 'BienOServicio'

from pedido p
FULL JOIN detalle_pedido dp ON dp.finopedido=p.finopedido  
FULL JOIN producto ON producto.fiprodid=dp.fiprodid
FULL JOIN  cliente  ON cliente.ficteid=p.ficteid and cliente.fidigitover=p.fidigitover
FULL  JOIN fisbitacoradocfiscal factura ON   factura.fcnoreferencia=p.finopedido  
FULL JOIN surtimientomoto  moto ON moto.fiNoPedido=p.finopedido
FULL JOIN control ON control.fiNoTienda=p.finotienda
FULL JOIN ventatarjeta ON ventatarjeta.fcmin=factura.fcnoreferencia
FULL JOIN movto_caja ON movto_caja.fiNoTransac=ventatarjeta.fiTranNo  
FULL  JOIN tipo_operacion ON tipo_operacion.fitipoop=movto_caja.fitipoop

